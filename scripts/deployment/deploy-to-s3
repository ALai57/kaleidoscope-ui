#!/bin/bash

aws s3 cp ./resources/public/js/compiled/andrewslai/andrewslai.js      s3://andrewslai/js/compiled/andrewslai/andrewslai.js
aws s3 cp ./resources/public/js/compiled/andrewslai/andrewslai.js.map  s3://andrewslai/js/compiled/andrewslai/andrewslai.js.map
aws s3 cp ./resources/public/js/compiled/andrewslai/editor.js          s3://andrewslai/js/compiled/andrewslai/editor.js
aws s3 cp ./resources/public/js/compiled/andrewslai/editor.js.map      s3://andrewslai/js/compiled/andrewslai/editor.js.map
aws s3 cp ./resources/public/js/compiled/andrewslai/manifest.edn       s3://andrewslai/js/compiled/andrewslai/manifest.edn
aws s3 cp ./resources/public/js/compiled/andrewslai/module-loader.edn  s3://andrewslai/js/compiled/andrewslai/module-loader.edn
aws s3 cp ./resources/public/js/compiled/andrewslai/module-loader.json s3://andrewslai/js/compiled/andrewslai/module-loader.json
#aws s3 cp ./resources/public/silent-check-sso.html s3://andrewslai/silent-check-sso.html
echo "Incrementing package version..."
npm version patch
